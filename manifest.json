{
   "name": "Youtube-music-player-chrome",
   "version": "0.0.1",
   "manifest_version": 2,
   "browser_action": {
      "default_icon": {
         "19": "icons/19x19.png",
         "38": "icons/38x38.png"
      },
      "default_title": "Music Player",
      "default_popup": "browseraction/popup.html"
   },
   "background": {
      "scripts": ["background.js"],
      "persistent": true
   }
}
